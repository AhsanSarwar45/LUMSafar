{
	"expo": {
		"name": "LUMSafar",
		"slug": "LUMSafar",
		"version": "0.1.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"jsEngine": "hermes",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#0088CC"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [ "**/*" ],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.ahsansarwar.LUMSafar"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#0088CC"
			},
			"package": "com.ahsansarwar.LUMSafar",
			"config": {
				"googleMaps": {
					"apiKey": "AIzaSyAQEUpohtyNtjBEdUQhPXjYdA3MyEySh3g"
				}
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"androidStatusBar": {
			"backgroundColor": "#000000"
		},
		"plugins": [
			[
				"expo-image-picker",
				{
					"photosPermission": "The app accesses your photos to let you share them with your friends."
				}
			]
		]
	}
}
